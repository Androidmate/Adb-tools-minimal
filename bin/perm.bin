#!/data/data/com.termux/files/usr/bin/bash -x

keydir="$(pwd)"
if [ ! -d "/data/ssh/root/.ssh" ]; then
  mkdir /data/ssh/root/.ssh
fi
if [ ! -d "/data/ssh/shell/.ssh" ]; then
  mkdir /data/ssh/shell/.ssh
fi
cp $PREFIX/tmp/keytemp/adbkey/authorized_keys /data/ssh/root/.ssh
cp $PREFIX/tmp/keytemp/adbkey/authorized_keys /data/ssh/shell/.ssh
keyfiles="$(ls $PREFIX/tmp/keytemp)"
cd /data/ssh/root/.ssh
chmod +r $keyfiles
cd /data/ssh/shell/.ssh
chmod +r $keyfiles
cd $keydir
